{
    "file_name": "project_dependency_config",
    "file_version": "1.0",
    "dependencies":
    [
        {
            "name": "Breathe",
            "website_url": "https://breathe.readthedocs.io/en/latest/",
            "download_url": "",
            "download_dir": "",
            "install_cmd": "pip",
            "install_arguments": [
                "install",
                "breathe"
            ],
            "ok_if_exists": "",
            "ok_if_executable": "pip",
            "executable_argument": "list",
            "executable_check_regex": "(breathe) "
        },
        {
            "name": "Sphinx",
            "website_url": "https://www.sphinx-doc.org/en/master/usage/quickstart.html",
            "download_url": "",
            "download_dir": "",
            "install_cmd": "pip install sphinx",
            "ok_if_exists": "",
            "ok_if_executable": "sphinx-build",
            "executable_argument": "--version",
            "executable_check_regex": "^\\S+ (.*)"
        },
        {
            "name": "Sphinx Doc Setup",
            "website_url": "https://www.sphinx-doc.org/en/master/usage/quickstart.html",
            "download_url": "",
            "download_dir": "",
            "install_cmd": "sphinx-quickstart",
            "install_arguments": 
            [
                "--project='${../../../../name}'",
                "--author='${PROJECT_AUTHOR}'",
                "-v '${../../../../version}'",
                "--release='${PROJECT_VERSION}'",
                "--language=en",
                "--no-makefile",
                "--no-batchfile",
                "--no-sep",
                "${PROJECT_ROOT}\\doc\\sphinx"
            ],
            "ok_if_exists": "${PROJECT_ROOT}\\doc\\sphinx",
            "ok_if_executable": "",
            "executable_argument": "",
            "executable_check_regex": ""
        }
    ]
}